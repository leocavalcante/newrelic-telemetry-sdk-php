{
    "name": "leocavalcante/newrelic-telemetry-sdk",
    "description": "Unofficial PHP library for sending telemetry data to New Relic",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "leocavalcante",
            "email": "lc@leocavalcante.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "analyze": "psalm",
        "test": "pest",
        "ci": [
            "@composer analyze",
            "@composer test"
        ]
    },
    "autoload": {
        "psr-4": {
            "NewRelic\\": "src/"
        },
        "files": [
            "src/Util/current_timestamp.php"
        ]
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "pestphp/pest": "^0.3.12",
        "vimeo/psalm": "^4.1"
    }
}
